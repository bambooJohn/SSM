<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bambooJohn.mapper.DeptMapper">
	
	<select id="getDeptByDid" resultType="dept">
		SELECT
		did,dname
		FROM
		dept
		WHERE did=#{did}
	</select>
	
</mapper>